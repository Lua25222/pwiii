@model List<pwiii.entidades.usuarioentidade>

<h1> Lista de Usuários</h1>







<table class="table">
    <thead>
        <tr>
            <th acope="col">Id</th>
            <th acope="col">Email</th>
            <th acope="col"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var usuario in Model)
        {
            <tr>
                <th>@usuario.Id</th>
                <td>@usuario.Email</td>
                <td></td>
            </tr>
        }
        
    
    </tbody>
</table>